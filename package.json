{
  "name": "React-TDD-Boilerplate",
  "version": "1.0.0",
  "description": "React TDD Boilerplate",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=development&&webpack-dev-server",
    "build": "set NODE_ENV=production&&webpack --config webpack.prod.config.js",
    "test": "set NODE_ENV=test&&mocha --ui tdd test/helpers/browser.js test/**/*.spec.js --compilers js:babel-core/register --recursive || true",
    "cover": "set NODE_ENV=test&&babel-node node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha -- --require test/helpers/browser.js test/**/*.spec.js --reporter dot || true"
  },
  "author": "Xiufeng Chen",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-preset-airbnb": "^2.1.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "browser-sync": "^2.16.0",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-minifier": "^3.0.2",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-template": "^5.1.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "normalize-css": "^2.3.1",
    "postcss-easy-import": "^1.0.1",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
